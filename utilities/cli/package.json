{
  "name": "nettie-app-generator",
  "module": "src/index.ts",
  "type": "module",
  "description": "CLI tool to generate new Nettie standalone apps and register them as submodules",
  "version": "0.1.0",
  "bin": {
    "nettie-create": "./bin/nettie-create.ts",
    "nettie": "./bin/nettie.ts"
  },
  "scripts": {
    "start": "bun run src/index.ts",
    "build": "bun build src/index.ts --outdir ./dist --target node",
    "test": "bun test"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@types/configstore": "^6.0.2",
    "@types/figlet": "^1.5.8",
    "@types/inquirer": "^9.0.7",
    "@types/node": "^20.10.5",
    "@types/open": "^6.2.1"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "chalk": "^5.3.0",
    "commander": "^11.1.0",
    "configstore": "^6.0.0",
    "figlet": "^1.7.0",
    "inquirer": "^9.2.12",
    "keytar": "^7.9.0",
    "octokit": "^3.1.2",
    "open": "^9.1.0",
    "simple-git": "^3.22.0"
  }
}